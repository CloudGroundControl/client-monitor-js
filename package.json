{
  "name": "@observertc/client-observer-js",
  "version": "1.0.0",
  "description": "ObserveRTC Client Integration Library",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "build": "tsc",
    "test": "jest --config jest.config.js",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "webrtc",
    "getStats",
    "observertc",
    "client",
    "integration"
  ],
  "author": "Balazs Kreith, Pallab Gain",
  "license": "MIT",
  "dependencies": {
    "@types/events": "^3.0.0",
    "@types/object-hash": "^2.2.1",
    "@types/uuid": "^8.3.4",
    "@types/ws": "^8.2.2",
    "bowser": "^2.11.0",
    "jest": "^27.4.7",
    "loglevel": "^1.8.0",
    "object-hash": "^2.2.0",
    "ts-jest": "^27.1.3",
    "ws": "^8.4.2"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "bufferutil": "^4.0.6",
    "jest-websocket-mock": "^2.3.0",
    "utf-8-validate": "^5.0.8"
  },
  "repository": "https://github.com/observertc/client-observer-js"
}
